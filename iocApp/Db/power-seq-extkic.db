record(bo, "BO-PS-EXK:enableSequence")
{
    field(DESC, "Starts the extraction kicker sequence.")
    field(RVAL, "0")
    field(PINI, "NO")
    field(ZNAM, "Disable") field(ZSV, "NO_ALARM")
    field(ONAM, "Enable") field(OSV, "NO_ALARM")
}

record(bi, "BO-PS-EXK:isSequenceReady")
{
    field(DESC, "Reads interlock status")
    field(RVAL, "0")
    field(PINI, "NO")
    field(ZNAM, "Not Ready")  field(ZSV, "NO_ALARM")
    field(ONAM, "Ready")	field(OSV, "NO_ALARM")
}

record(longin, "BO-PS-EXK:getSequenceCountdown")
{
	field(DESC, "Reads the countdown timer.")
	field(VAL, "0")
	field(PINI, "NO")
}

